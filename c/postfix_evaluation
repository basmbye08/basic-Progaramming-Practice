#include<stdio.h>
#include<ctype.h>
#include<math.h>
#define MAX 100

int stack[MAX], top=-1;

void push(int val) {
    stack[++top]=val;
}

int pop() {
    return stack[top--];
}

int evaluatePostfix(char exp[]) {
    int i, op1, op2;
    for(i=0; exp[i]!='\0'; i++) {
        if(isdigit(exp[i])) {
            push(exp[i]-'0');
        }
        else {
            op2=pop();
            op1=pop();
            switch(exp[i]) {
                case '+': push(op1+op2); break;
                case '-': push(op1-op2); break;
                case '*': push(op1*op2); break;
                case '/': push(op1/op2); break;
                case '^': push(pow(op1, op2)); break;
            }
        }
    }
    return pop();
}

int main() {
    char exp[MAX];
    printf("Enter postfix expression: ");
    scanf("%s", exp);
    int result=evaluatePostfix(exp);
    printf("Result: %d\n", result);
    return 0;
}
